{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Current TS Test File",
      "type": "pwa-node",
      "request": "launch",
      "program": "${workspaceRoot}/typescript/node_modules/mocha/bin/_mocha",
      "args": ["-r", "ts-node/register", "${workspaceRoot}/${relativeFile}"],
      "cwd": "${workspaceRoot}/typescript",
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ]
    },
    {
      "name": "GO Launch",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}",
      "env": {},
      "args": []
    }
  ]
}
